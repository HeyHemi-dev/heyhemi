{
  "name": "heyhemi",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "diagrams:build": "node scripts/render-mermaid.mjs",
    "preview": "astro build && wrangler pages dev",
